<% @todo_lists.each do |todo_list| %>
  <div class="clearfix">
    <h2 class="todo-title">
      <%= link_to todo_list.title, todo_list %>
    </h2>
    <p class="todo-sub-title">
      <%= todo_list.description %>
    </p>
  </div>
<% end %>

<div class="navigation">
  <%= link_to "Create New Todo List", new_todo_list_path %>
</div>

